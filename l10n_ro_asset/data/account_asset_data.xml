<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
		
		<!--Asset Inventory Sequence -->
        
		<record id="ir_sequence_invasset" model="ir.sequence">
            <field name="name">Inventar Mijloace Fixe</field>
            <field eval="6" name="padding"/>
            <field name="implementation" eval="'no_gap'"/>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="prefix">INV/</field>
        </record>
        
		<!--Account Journal Sequence -->
        
        <record id="ir_sequence_amort" model="ir.sequence">
            <field name="name">Jurnal amortizare</field>
            <field eval="4" name="padding"/>
            <field name="implementation" eval="'no_gap'"/>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="prefix">AMORT/%(year)s/</field>
        </record>
        
        <!--Account Journal-->
		
        <record id="amort_journal" model="account.journal">
            <field name="name">Jurnal amortizare</field>
            <field name="code">AMORT</field>
            <field name="type">general</field>
            <field name="sequence_id" ref="ir_sequence_amort"/>
            <field name="default_credit_account_id" model="account.account" search="[('code','=','681100')]"/>
            <field name="default_debit_account_id" model="account.account" search="[('code','=','681100')]"/>
            <field name="user_id" ref="base.user_root"/>
        </record> 
        
    </data>
</openerp>
